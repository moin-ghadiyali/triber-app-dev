  "version": "0.2.0",
  "configurations": [
    {
        "name": "Attach to packager",
        "cwd": "${workspaceRoot}",
        "type": "reactnative",
        "request": "attach",
        "sourceMaps": true,
    },
    {
      "name": "Debug Android",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "android"
    },
    {
      "name": "Run Android",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "enableDebug": false
    }
  ]
}
